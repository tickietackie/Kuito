{
    // Use IntelliSense to learn about possible attributes. Hover to view
    // descriptions of existing attributes. For more information, visit:
    // https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Launch Firefox",
            "url": "http://localhost/8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Attach to packager",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "attach"
        },
        {
            "type": "firefox",
            "request": "attach",
            "name": "Attach",
            "url": "http://localhost:19001",
            "webRoot": "${workspaceFolder}"
        }, {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }

    ]
}